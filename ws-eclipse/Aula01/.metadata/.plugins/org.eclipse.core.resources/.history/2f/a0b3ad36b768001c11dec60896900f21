import java.util.Scanner;

public class exercicio03 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int A, B, C, D, dif;
		
		A = sc.nexInt();
		B = sc.nextInt();
		C = sc.nextInt();
		D = sc.nextInt();
		dif = sc.nextInt();
		
		dif = A * B - C * D;
		
		System.out.printf(" Diferença = " + dif);
		
		sc.close();
	}

}
