import java.util.Scanner;
public class exercicio03 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int dif;
		
		int A = sc.nexInt();
		int B = sc.nexInt();
		int C = sc.nextInt();
		int D = sc.nextInt();
		
		dif = A * B - C * D;
		
		System.out.println("Diferença:" + dif);
		sc.close();

	}

}
