import java.util.Scanner;

public class exercicio03 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int A = sc.nexInt();
		int B = sc.nextInt();
		int C = sc.nextInt();
		int D = sc.nextInt();
		int dif = sc.nextInt();
		
		dif = A * B - C * D;
		
		System.out.printf(" Diferença = " + dif);
		
		sc.close();
	}

}
